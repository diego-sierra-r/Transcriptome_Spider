bind_DE <- function(by) {
if (by == "wide") {
x <- list()
for (i in DESeq2_diet_wide) {
x[[length(x)+1]] <- as.data.frame(i[1])
}
DE <- list()
for (j in x) {
DE[[length(DE)+1]]<- as.data.frame(j)
}
DE <-bind_rows(DE)
DE <- DE %>% bind_rows(as.data.frame(DESeq2_sex[1]))
return(DE)
}else if (by == "granulate") {
x <- list()
for (i in DESeq2_diet_granulate) {
x[[length(x)+1]] <- i[1]
}
DE <- list()
for (j in x) {
DE[[length(DE)+1]]<- as.data.frame(j)
}
DE <-bind_rows(DE)
DE <- DE %>% bind_rows(as.data.frame(DESeq2_sex[1]))
return(DE)
}
}
